@model IEnumerable<MovieRental1.Models.CatClass>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>  

    
2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.newMovie.MovieName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.newCustomer.CustomerPhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.newRecord.ReturnDate)
        </th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.newMovie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.newCustomer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.newRecord)
            </td>
        </tr>
    }
</table>
